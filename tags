detect	lib/yandex-translator.rb	/^  def detect(text)$/;"	f	class:Yandex
get_langs	lib/yandex-translator.rb	/^  def get_langs$/;"	f	class:Yandex
translate	lib/yandex-translator.rb	/^  def translate(text, *lang)$/;"	f	class:Yandex
